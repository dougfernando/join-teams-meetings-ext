{
    "$schema": "https://www.raycast.com/schemas/extension.json",
    "name": "join-teams-meetings-ext",
    "title": "Join Teams Meetings",
    "description": "List today's meeting and select one to join",
    "icon": "icon.png",
    "author": "dfs",
    "categories": [
        "Productivity Tools"
    ],
    "license": "MIT",
    "platforms": [
        "Windows"
    ],
    "commands": [
        {
            "name": "find-meetings",
            "title": "Find today's teams meetings",
            "description": "List today's meeting and select one to join",
            "mode": "view"
        }
    ],
    "preferences": [
        {
            "name": "emptyStart",
            "title": "Should start with empty list?",
            "description": "Decides if the search should start with an empty list or not",
            "type": "checkbox",
            "required": false,
            "default": false
        }
    ],
    "dependencies": {
        "@raycast/api": "^1.40.0",
        "@raycast/utils": "^1.4.8"
    },
    "devDependencies": {
        "@raycast/eslint-config": "^1.0.6",
        "@types/node": "18.18.4",
        "@types/react": "19.0.10",
        "eslint": "^8.51.0",
        "prettier": "^3.0.3",
        "typescript": "^5.2.2"
    },
    "scripts": {
        "build": "ray build -e dist",
        "dev": "ray develop",
        "fix-lint": "ray lint --fix",
        "lint": "ray lint",
        "publish": "ray publish"
    }
}
